{
    "name": "mtymek/blast-reflection-factory",
    "description": "Universal auto-wiring factory for Zend ServiceManager.",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Mateusz Tymek",
            "email": "mtymek@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "zendframework/zend-servicemanager": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Blast\\ReflectionFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blast\\Test\\ReflectionFactory\\": "test"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
