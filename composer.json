{
    "name": "mtymek/blast-reflection-factory",
    "description": "Universal factory for Zend ServiceManager",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Mateusz Tymek",
            "email": "mtymek@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-servicemanager": "^3.1",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Blast\\ReflectionFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blast\\Test\\ReflectionFactory\\": "test"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
